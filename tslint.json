{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "max-line-length": {
        "options": [120]
    },
    "no-console": false,
    "interface-name": false,
    "no-var-requires": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  }
}
